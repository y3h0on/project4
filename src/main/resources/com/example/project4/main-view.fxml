<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="549.0" prefWidth="604.0" spacing="20.0" style="-fx-background-color: #b3ae9a;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project4.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label prefHeight="72.0" prefWidth="254.0" text="Rutgers Cafe" textFill="#e44747">
            <font>
                <Font name="Brush Script MT Italic" size="53.0" />
            </font>
        </Label>
        <GridPane prefHeight="430.0" prefWidth="564.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#switchToDonutPage" style="-fx-background-color: black;" text="order donuts" textFill="#fcfcfc" GridPane.columnIndex="1" GridPane.halignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="196.0" fitWidth="196.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@donut.jpg" />
                            </image></ImageView>
                    </graphic>
                    <GridPane.margin>
                        <Insets bottom="20.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="coffeeButton" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#switchToCoffeePage" prefHeight="147.0" prefWidth="274.0" style="-fx-background-color: black;" text="order coffee" textAlignment="JUSTIFY" textFill="#fffffc" underline="true" wrapText="true" GridPane.halignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="117.0" fitWidth="255.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@coffee.jpg" />
                            </image></ImageView>
                    </graphic>
                    <GridPane.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" />
                    </GridPane.margin>
                </Button>
                <Button contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#switchToBasketPage" style="-fx-background-color: black;" text="your order" textFill="WHITE" GridPane.halignment="CENTER" GridPane.rowIndex="1">
                    <graphic>
                        <ImageView fitHeight="200.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@basket.jpg" />
                            </image></ImageView>
                    </graphic>
                    <GridPane.margin>
                        <Insets top="20.0" />
                    </GridPane.margin>
                </Button>
                <Button contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#switchToStoreOrderPage" style="-fx-background-color: black;" text="store orders" textFill="WHITE" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
                    <graphic>
                        <ImageView fitHeight="200.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@order.jpg" />
                            </image></ImageView>
                    </graphic>
                    <GridPane.margin>
                        <Insets left="30.0" right="10.0" top="20.0" />
                    </GridPane.margin>
                </Button>
            </children>
        </GridPane>
    </children>
</VBox>
