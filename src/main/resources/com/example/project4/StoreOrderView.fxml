<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" cacheShape="false" prefHeight="549.0" prefWidth="604.0" spacing="20.0" style="-fx-background-color: #b3ae9a;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project4.StoreOrderController">
    <children>
      <Button mnemonicParsing="false" onAction="#switchToMainPage" text="Back" />
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <Label prefHeight="92.0" prefWidth="471.0" text="Store Orders">
                    <font>
                        <Font name="MT Extra" size="58.0" />
                    </font>
                    <padding>
                        <Insets left="140.0" />
                    </padding>
                </Label>
            </children>
        </HBox>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <ComboBox fx:id="selectOrder" focusTraversable="false" prefHeight="43.0" prefWidth="150.0" promptText="Select an Order" GridPane.halignment="CENTER" />
                <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
                    <children>
                        <Label prefHeight="22.0" prefWidth="104.0" text="total price:">
                            <padding>
                                <Insets right="10.0" />
                            </padding>
                            <font>
                                <Font name="MT Extra" size="18.0" />
                            </font>
                            <HBox.margin>
                                <Insets top="5.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="totalPrice" editable="false" focusTraversable="false" prefHeight="33.0" prefWidth="178.0" text="\$0.00">
                            <font>
                                <Font name="MT Extra" size="18.0" />
                            </font>
                        </TextField>
                    </children>
                </HBox>
            </children>
        </GridPane>
        <ListView fx:id="orderList" focusTraversable="false" prefHeight="261.0" prefWidth="564.0">
            <VBox.margin>
                <Insets left="20.0" right="20.0" />
            </VBox.margin>
        </ListView>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button fx:id="cancelOrder" focusTraversable="false" mnemonicParsing="false" prefHeight="34.0" prefWidth="179.0" text="Save/Export Order" GridPane.halignment="CENTER">
                    <font>
                        <Font name="MT Extra" size="18.0" />
                    </font>
                </Button>
                <Button fx:id="seOrder" focusTraversable="false" mnemonicParsing="false" text="Cancle Order" GridPane.columnIndex="1" GridPane.halignment="CENTER">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
            </children>
            <padding>
                <Insets bottom="15.0" />
            </padding>
        </GridPane>
    </children>
</VBox>
